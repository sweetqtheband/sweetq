{
  "name": "dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prod:dev": "dotenv -e .env.production next dev",
    "prod:build": "dotenv -e .env.production next build",
    "prod:start": "dotenv -e .env.production next start",
    "lint": "next lint",
    "format": "prettier --write .",
    "update:followers": "dotenv -e .env.production tsx updates/followers.ts",
    "update:images": "dotenv -e .env.production tsx updates/images.ts",
    "update:catalog": "dotenv -e .env.production tsx updates/userCatalog.ts",
    "update:instagram": "dotenv -e .env.production tsx updates/instagram.ts",
    "update:names": "dotenv -e .env.production tsx updates/gemini.ts",
    "update:test": "dotenv -e .env.production tsx updates/test.ts",
    "update": "pnpm run update:followers && pnpm run update:instagram && pnpm run update:catalog",
    "messages": "dotenv -e .env.production tsx updates/messages.ts",
    "sq": "node scripts/swq.js",
    "expose": "ngrok http 3000  --url=overly-awaited-leech.ngrok-free.app"
  },
  "bin": {
    "swq": "scripts/swq.js"
  },
  "dependencies": {
    "@google/genai": "^0.8.0",
    "@aws-sdk/client-s3": "^3.883.0",
    "@carbon/charts": "^1.23.17",
    "@carbon/charts-react": "^1.23.17",
    "@carbon/react": "1.83.0",
    "@carbon/styles": "1.83.0",
    "@emoji-mart/data": "^1.2.1",
    "@emoji-mart/react": "^1.1.1",
    "@types/crypto-js": "^4.2.2",
    "@types/emoji-mart": "^3.0.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/uuid": "^10.0.0",
    "@types/qs": "^6.14.0",
    "axios": "^1.11.0",
    "crypto-js": "^4.2.0",
    "emoji-mart": "^5.6.0",
    "formidable": "^3.5.4",
    "i18next": "^25.5.2",
    "i18next-http-backend": "^2.7.3",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.19.0",
    "next": "14.2.3",
    "next-i18next": "^15.4.2",
    "qs": "^6.14.0",
    "react": "^18.3.1",
    "react-contenteditable": "^3.3.7",
    "react-dom": "^18.3.1",
    "react-i18next": "^15.7.3",
    "sass-loader": "^16.0.5",
    "sharp": "^0.33.5",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.19.13",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/yargs": "^17.0.33",
    "dotenv": "^16.6.1",
    "dotenv-cli": "^8.0.0",
    "eslint": "^9.35.0",
    "eslint-config-next": "^15.5.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "fs-extra": "^11.3.1",
    "p-limit": "^6.2.0",
    "prettier": "^3.6.2",
    "puppeteer": "^24.19.0",
    "sass": "^1.92.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "yargs": "^17.7.2"
  }
}
