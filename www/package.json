{
  "name": "sweetq",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "api": "nodemon",
    "api:start": "NODE_ENV=development npm-run-all api:build api:server",
    "api:clean": "rimraf ./dist/api ./transpiled",
    "api:clean:transpiled": "rimraf ./transpiled",
    "api:media": "rsync -avz ./api/media ./dist/",
    "api:esbuild": "esbuild ./transpiled/server.js --platform=node --bundle --outfile=dist/api/server.js",
    "api:build": "npm-run-all api:clean api:transpile api:esbuild api:media api:clean:transpiled",
    "api:transpile": "babel ./api --out-dir transpiled",
    "api:server": "node ./dist/api/server.js",
    "start": "npm run serve & npm run api",
    "serve": "ng serve",
    "build": "npm run api:build & npm run ng:build",
    "ng:build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "@types/crypto-js": "^4.1.2",
    "angulartics2": "^12.2.0",
    "buffer": "^6.0.3",
    "cors": "^2.8.5",
    "crypto-js": "^4.1.1",
    "dexie": "^3.2.3",
    "express": "^4.18.2",
    "puppeteer": "^21.5.0",
    "querystring": "^0.2.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "url": "^0.11.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.6",
    "@angular/cli": "~15.2.6",
    "@angular/compiler-cli": "^15.2.0",
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-syntax-import-attributes": "^7.25.6",
    "@babel/preset-env": "^7.22.20",
    "@types/jasmine": "~4.3.0",
    "@types/node": "^18.16.3",
    "esbuild": "^0.19.4",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "node-fetch": "^3.3.2",
    "nodemon": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^5.0.5",
    "typescript": "~4.9.4"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-import-attributes"
    ]
  },
  "nodemonConfig": {
    "exec": "npm run api:start",
    "watch": [
      "api/*"
    ]
  }
}
